<h2> Create new subject as per standard</h2>

<%= form_for(@subject) do |f| %>

	<%= f.label :name %><br/>
	<%= f.text_field :name %><br/>
	<%= f.label :description %><br/>
	<%= f.text_field :description %><br/>
	<%= f.label :credit %><br/>
	<%= f.text_field :credit %><br/>
	<%= f.label :teacher_id %><br/>
	<%= f.text_field :teacher_id %><br
	<%= f.label :standard_id %><br/>
	<%= f.text_field :standard_id %><br
	<%= f.label :exam_id %><br/>
	<%= f.text_field :exam_id %><br
	<%= f.label :student_id %><br/>
	<%= f.text_field :student_id %><br
	<%= f.label :result_id %><br/>
	<%= f.text_field :result_id %><br

	<div class="actions">
    <%= f.submit "Create" %><br/> 
  </div> 


	<% end %>